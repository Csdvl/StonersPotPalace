    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "keyboard-key": "^1.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-datepicker": "^1.5.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.1",
    "react-load-script": "0.0.6",
    "react-places-autocomplete": "^7.1.2",
    "react-pose": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.1",
    "react-redux-toastr": "^7.2.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-firestore": "^0.5.4",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "resolve": "1.6.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.81.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "flow": "flow check src",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "serve": "serve -s build",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "test": "npm run build:testConfig cypress run",
    "test:open": "npm run build:testConfig && cypress open",
    "test:stage": "npm run test -- --env envName=stage",
    "test:open:stage": "npm run test:open -- --env envName=stage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "flow"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "babel-cli": "^6.26.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.4",
    "cypress-firebase": "^0.1.0",
    "faker": "^4.1.0",
    "firebase-admin": "^6.5.0",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.2.2",
    "firebase-tools-extra": "0.0.7",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "json-server": "^0.12.2",
    "node-sass": "^4.8.3",
    "react-hot-loader": "^4.2.0",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.19.0"
  }
}
